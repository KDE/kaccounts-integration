#: src/kcm/ui/AccountDetails.qml:20
#, kde-format
msgid "Account Details"
msgstr "계정 정보"

#: src/kcm/ui/AccountDetails.qml:59 src/kcm/ui/main.qml:48
#, kde-format
msgid "%1 (%2)"
